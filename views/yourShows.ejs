<h3>Your Shows</h3>

<% user.shows.forEach(function(show) { %>
<div class="container">	
	<div class="row">
		<div class="col s5">
			<img src = "<%= show.image %>">
		</div>
		<div class="col s7">
			<h5><%= show.artist %></h5>
			<p>Show: <%= show.title %> </p>
			<p>Venue: <%= show.venue %></p>
			<p>Date: <%= show.date %></p>
			<a href="/shows/<%= show.id %>" class='btn delete-show'>I'm Out</a>
			<a href="/reviews/<% show.id %>" class='btn'>Write Review</a>
			<a href="/rides/<% show.id %>" class='btn'>Offer to Drive</a>
			<br>
			<br>
		</div>
	</div>
</div>
<% }); %>

<script type="text/javascript" src="/js/script.js"></script>