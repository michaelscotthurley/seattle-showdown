<h1>Shows You're Attending</h1>

<% user.shows.forEach(function(show) { %>
<div class="container">	
	<div class="row">
		<div class="col s6">
			<img src = "<%= show.image %>">
		</div>
		<div class="col s6">
			<h5><%= show.artist %></h5>
			<p>Show: <%= show.title %> </p>
			<p>Venue: <%= show.venue %></p>
			<p>Date: <%= show.date %></p>
			<a href="/shows/<%= show.id %>" class='btn delete-show'>I'm Out</a>
		</div>
	</div>
</div>
<% }); %>

<script type="text/javascript" src="/js/script.js"></script>