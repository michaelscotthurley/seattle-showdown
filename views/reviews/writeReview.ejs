<% shows.forEach(function(show) { %>
<h4 class="heading"><%= show.title %></h4>
<h5 class="center"><%= show.date %></h5>
<% }); %>
<div class="container">
    <form method="POST" role="form" id="newreview">
      <div class="row">
        <div class="col s8">
          <p class="flow-text"><strong>So...what did you think?</strong></p>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s8">
            <input type="text" class="form-control" name="reviewTitle" placeholder="ex: Best.Show.Ever." >
            <label for="reviewTitle">Title</label>
          </div>
        </div>
        <div class="row">
        <div class="input-field col s8">
            <textarea name="reviewBody" form="newreview" id="body" style="width: 550px; height: 200px;"></textarea>
            <label for="review">
              Tell us about the show! Were you thoroughly rocked and or rolled? How about the venue...classy? Divey? Somewhere in between? Let your fellow Seattleites know!
            </label>
        </div>
      </div>
    <button type="submit" class="btn btn-primary">SUBMIT REVIEW</button>
  </form>
</div>
