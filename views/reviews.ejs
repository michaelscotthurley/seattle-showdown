<h4>Show Reviews</h4>

<% reviews.forEach(function(review) { %>
	<div class="container">
		<div class="row">
			<div class="col s12">
				<h6><%= review.show.title %></h6>
				<p><%= review.title %></p>
				<p><%= review.body %></p>
				<p><%= review.user.name %></p>
			</div>
		</div>
	</div>

<% }); %>