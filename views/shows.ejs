<h3>Shows People are Going To:</h3>

<% allshows.forEach(function(show) { %>
	<div class="container">
		<div class="row">
			<div class="col s6">
				<img src = "<%= show.image %>">
			</div>
			<div class="col s6">
				<h5>Artist: <%= show.artist %></h5>
				<p>Title: <%= show.title %> </p>
				<p>Venue: <%= show.venue %></p>
				<p>Date: <%= show.date %></p>
			</div>
		</div>
	</div>
<% }); %>